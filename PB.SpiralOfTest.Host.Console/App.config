<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <system.serviceModel>
    <behaviors>
    </behaviors>
    <bindings>
      <netTcpBinding>
        <binding name="TransactionalReliableTCP" transactionFlow="true">
          <reliableSession enabled="true" />
        </binding>
      </netTcpBinding>
    </bindings>
    <services>
      <service name="PB.SpiralOfTest.Manager.Party.PartyManager">
        <endpoint 
          name="partyManager" 
          address="net.tcp://localhost:8001/PartyManager" 
          binding="netTcpBinding" 
          contract="PB.SpiralOfTest.Manager.Party.IPartyManager" 
          bindingConfiguration="TransactionalReliableTCP" />
      </service>
    </services>
  </system.serviceModel>
  <appSettings>
    <add key="BaseAddress" value="net:tcp://localhost"/>
  </appSettings>
</configuration>